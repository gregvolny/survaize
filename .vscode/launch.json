    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: survaize convert",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/survaize/main.py",
            "console": "integratedTerminal",
            "args": [
            "convert",
            "examples/PopstanHouseholdSurvey/PopstanHouseholdQuestionnaire.json",
            "output/PopstanHouseholdSurvey",
            "--format",
            "cspro",
            ],
            "justMyCode": false
        },
        {
            "name": "Python Debugger: survaize ui",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/survaize/main.py",
            "console": "integratedTerminal",
            "args": ["ui", "--no-browser"],
            "justMyCode": false
        }
    ]
}